{
  "name": "create-visactor-app",
  "version": "0.1.3",
  "description": "Create web application with VisActor",
  "keywords": [
    "create-visactor-app",
    "visactor",
    "vchart",
    "vtable",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mengxi-ream/create-visactor-app.git",
    "directory": "cli"
  },
  "license": "MIT",
  "author": "MengXi",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "visactor": "./dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "pnpm-lock.yaml",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --write",
    "format:check": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --check",
    "link-cli": "pnpm link --global",
    "lint": "eslint . --report-unused-disable-directives",
    "relink-cli": "pnpm uninstall --global create-visactor-app && pnpm link --global",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@clack/core": "^0.4.1",
    "@clack/prompts": "^0.10.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "ora": "^8.2.0",
    "type-fest": "^4.35.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.5",
    "tsup": "^8.3.6",
    "@repo/eslint-config": "workspace:*",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
